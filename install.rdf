<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>enhancedmiddleclick@senicar.net</em:id>
    <em:version>0.4.8</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>

    <!-- Target Application this extension can install into,
         with minimum and maximum supported versions. -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id> <!-- Firefox -->
        <em:minVersion>38.0</em:minVersion>
        <em:maxVersion>40.0</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Options -->
    <em:optionsURL>chrome://enhancedmiddleclick/content/options.xul</em:optionsURL>
    <em:optionsType>2</em:optionsType>

    <!-- Front End MetaData -->
    <em:creator>Senicar</em:creator>
    <em:name>Enhanced Middle Click</em:name>
    <em:description>Display history menu, tab list menu or use other available actions with middle mouse button.</em:description>

	<!-- localized -->
	<em:localized>
		<Description>
			<em:locale>en-US</em:locale>
			<em:creator>Senicar</em:creator>
			<em:name>Enhanced Middle Click</em:name>
			<em:description>Display history menu, tab list menu or use other available actions with middle mouse button.</em:description>
		</Description>
	</em:localized>
  </Description>
</RDF>
